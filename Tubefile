#!/usr/bin/env groovy
/*
 * Tubefile
 * Tube
 *
 * Builds the current branch using the version of Tube loaded by the @Library
 * annotation (master by default).
 */

@Library('Tube@tube-dsl') _

tube {

    label = "node1"

    env = [
        FOO : 42,
        BAR : "YASS"
    ]

    before_script = "echo before"
    script = 'echo after $FOO'
    after_script = 'echo done now'

}

